{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"blockTypes": [],
	"slug": "footer",
	"title": "Part: Footer",
	"styles": {
		"color": {
			"background": "var:preset|color|base-2",
			"text": "var:preset|color|contrast-3"
		},
		"typography": {
			"fontSize": "var:preset|font-size|small"
		},
		"elements": {
			"heading":{
				"color": {
					"text": "var:preset|color|base"
				}
			},
			"link":{
				"color": {
					"text": "var:preset|color|brand-3"
				},
				"typography": {
					"textDecoration": "none"
				},
				":hover":{
					"color": {
						"text": "var:preset|color|base"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			}
		},
		"border": {
			"top": {
				"color": "var:preset|color|base-3",
				"style": "solid",
				"width": "1px"
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var:preset|color|base-2",
					"text": "var:preset|color|contrast-3"
				}
			}
		}
	}
}
