{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"blockTypes": ["core/group", "core/columns", "core/column"],
	"slug": "section-brand-2",
	"title": "[Brand-2]",
	"styles": {
		"color": {
			"background": "var:preset|color|brand-2",
			"text": "var:preset|color|base"
		},
		"elements": {
			"heading":{
				"color": {
					"text": "var:preset|color|base"
				}
			},
			"link":{
				"color": {
					"text": "var:preset|color|base-2"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover":{
					"color": {
						"text": "var:preset|color|base-3"
					},
					"typography": {
						"textDecoration": "underline"
					}
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var:preset|color|brand",
					"text": "var:preset|color|base"
				},
				"css": "/** variations styled via scss */"
			},
			"core/separator":{
				"color": {
					"text": "var:preset|color|brand-3"
				}
			}
		}
	}
}
